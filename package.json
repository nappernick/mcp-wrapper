{
  "name": "mcp-wrapper",
  "module": "index.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./providers/*": {
      "import": "./dist/providers/*.js",
      "types": "./dist/providers/*.d.ts"
    }
  },
  "scripts": {
    "build": "rm -rf dist && bun run tsc",
    "start": "node dist/exampleApp.js",
    "test": "jest",
    "dev": "ts-node src/exampleApp.ts",
    "eval": "bun run src/scripts/run-evals.ts",
    "prepublish": "bun run build",
    "start-server": "bun ./src/scripts/start-server.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.10.1",
    "@types/uuid": "^10.0.0",
    "ts-node": "^10.9.2"
  },
  "keywords": [
    "MCP",
    "Model Context Protocol",
    "OpenAI",
    "Anthropic",
    "TypeScript",
    "LLM"
  ],
  "peerDependencies": {
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "node-cache": "^5.1.2",
    "winston": "^3.9.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@modelcontextprotocol/sdk": "latest",
    "ajv": "^8.17.1",
    "axios": "^1.7.9",
    "colorette": "^2.0.20",
    "dotenv": "^16.4.7",
    "openai": "^4.76.0",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  }
}